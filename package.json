{
  "name": "electron-vite-ts-stackter",
  "version": "1.0.0",
  "description": "An Electron + Vue 3 starter project setup with Vite, Typescript, Vue-Router, Pinia, Eslint, Prettier, Tailwind CSS, and some custom preferences.",
  "author": "Shamim Hossain Twitter<@shamscorner>",
  "license": "MIT",
  "private": true,
  "main": "dist-electron/main/index.js",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "app:dev": "vite",
    "app:build": "vue-tsc --noEmit && vite build && electron-builder",
    "app:preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.11.3",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.16"
  }
}
